      pauseOnHover: false,

      responsive: [

      {

        breakpoint: 1199,

        settings: {

        arrows: false,

        dots: true,

        }

      }

      

      ]

    });



  $('.brandslider').slick({

        dots: false,

        infinite: true,

        speed: 500,

        slidesToShow: 1,

        slidesToScroll: 1,

        swipeToSlide:true,

